{
  "name": "nexer-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint --fix --max-warnings 10 \"src/**/*.{js,ts,vue}\"",
    "prettier:check": "prettier --check '**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}'",
    "prettier:write": "prettier --write '**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}'"
  },
  "dependencies": {
    "@fawmi/vue-google-maps": "^0.9.67",
    "@types/nodemailer": "^6.4.7",
    "@vuepic/vue-datepicker": "^4.0.0",
    "aframe": "^1.4.1",
    "ar.js": "^2.2.2",
    "dayjs": "^1.11.7",
    "emailjs-com": "^3.2.0",
    "nodemailer": "^6.6.3",
    "pinia": "^2.0.33",
    "swiper": "^9.0.5",
    "three": "^0.150.1",
    "vue": "^3.2.36",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-aframe": "^0.0.0",
    "vue-tsc": "^1.0.12"
  }
}
